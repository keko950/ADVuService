        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
		"outDir": "services",
		"sourceMap": false,
		"allowJs": false,
		"target": "es5",
		"lib": ["es5", "es6", "dom"]
	},
	"exclude": [
        "node_modules",
        "**/*.spec.ts"
	],
	"include": [
		"./src/**/*"
	]
}
